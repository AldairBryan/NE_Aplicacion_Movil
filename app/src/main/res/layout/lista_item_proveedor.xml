<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="#b2b2ff"
        android:padding="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/viewIdProveedor"
            android:layout_width="340dp"
            android:layout_height="wrap_content"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/viewNombreProveedor"
            android:layout_width="340dp"
            android:layout_height="wrap_content"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <TextView
            android:id="@+id/viewRUCProveedor"
            android:layout_width="340dp"
            android:layout_height="wrap_content"
            android:textSize="24sp" />
        <TextView
            android:id="@+id/viewFKTPProveedor"
            android:layout_width="340dp"
            android:layout_height="wrap_content"
            android:textSize="24sp" />
        <TextView
            android:id="@+id/viewFKPaisProveedor"
            android:layout_width="340dp"
            android:layout_height="wrap_content"
            android:textSize="24sp" />
        <TextView
            android:id="@+id/viewERProveedor"
            android:layout_width="340dp"
            android:layout_height="wrap_content"
            android:textSize="24sp" />
        <ImageView
            android:id="@+id/listaItemProveedorCheck"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_tick_point" />
    </LinearLayout>
</LinearLayout>